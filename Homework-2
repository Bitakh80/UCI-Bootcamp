
'2015’
Sub easy()
Dim ticker as string
Dim vol as double
Vol=0
Dim summary table row as integer
Dim year open as double 
Dim year close as double
Cell(1,9).value = “Ticker”
Cell(1,10).value = “Yearly change”
Cell(1,11).value =”Total stock vol”
Summary table row = 2
For I = 2 to 760192
If year open = 0 then
Year open  = cell(1,3).value
End If
If cell(I-1,1) = Cells(I,1) and cells(I+1).value <> cells.value then
Year close = cell(I,6).value
Yearly change = year close-year open
Ticker = cells9I,1).value
Vol = vol + cells(1,7).value
Range(“j” & summary table row).value = yearly change
Range (“I” & summary table row).value = tiker
Range(“L” & summary table row).value=vol
Summary table row = summary table row + 1
Vol = 0
Else
Vol = vol + cells(1,7).value
End if
Next I
End sub


